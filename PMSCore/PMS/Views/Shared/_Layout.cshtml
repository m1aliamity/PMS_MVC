<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PMS</title>
    <link rel="stylesheet" href="~/lib/Script/Global/theme/css/style-2.3.0.css">
    <link rel="stylesheet" href="~/lib/CSS/Global/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/CSS/Global/jquery-ui.min.css" />
    <link rel="stylesheet" href="~/lib/Script/DataTable/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/Script/DataTable/css/responsive.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/Script/DataTable/css/font-awesome.min.css" />
    <script src="~/lib/Script/Global/jquery-3.5.1.min.js"></script>
    <script src="~/lib/Script/DataTable/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/Script/DataTable/js/dataTables.responsive.min.js"></script>

</head>
<body>
    @Html.RenderPartialAsync("_Header");
    <div id="PageHeaderText">
        <span>Testing pathology Lab</span>
    </div>
    @*<div id="PageHeaderText" class="col-sm-6">
            <span>Master Data List</span>
        </div>*@

    <main id="main">
        <div class="container">
            <div class="page-content">
                <input id="RowId" type="hidden" />
                @RenderBody()
            </div>
        </div>
    </main>
    <footer id="" style="position: fixed;left: 0;bottom: 0;width: 100%;text-align: center;border-top: 1px solid #3477b3;">
        <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
    </footer>
    <script src="~/lib/Script/BootStrap/bootstrap.js"></script>
    <script src="~/lib/Script/BootStrap/popper.min.js"></script>
    <script src="~/lib/Script/Global/jquery-ui.min.js"></script>

    @*<script src="~/lib/Script/Global/theme/js/main-2.3.0.js"></script>*@
    <script>
        $(document).ready(function () {
        });
    </script>
    <script>
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>

